{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": [
        "transform-react-jsx-source",
        ["module-resolver",{
              "root": ["./"],
              "alias": {
                "components": "./src/components",
                "config": "./src/config",
                "native-base-theme": "./native-base-theme",
                "navigator": "./src/navigator",
                "pages": "./src/pages",
                "assets": "./src/assets",
                "actions": "./src/actions"
              }
            }
          ]
      ]
    }
  }
}
